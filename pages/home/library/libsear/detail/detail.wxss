.body {
  display: flex;
  justify-content: center;
  flex-direction: column;
  align-items: center;
}

.one {
  flex-direction: column;
  background: #fff;
  margin-top: 30rpx;
  width: 690rpx;
  height: 500rpx;
  box-shadow: 0px 4rpx 16rpx 0px rgba(140, 160, 181, 0.2);
  border-radius: 12rpx;
  display: flex;
  box-sizing: border-box;
}

.title {
  font-weight: 900;
  font-size: 30;
