.header{
  display: flex;
  flex-direction: row;
}
.rollCon {
  /*position: fixed;*/
  top: 0;
  left: 0;
  width: 100%;
  height: 60rpx;
  z-index: -2;
  background: #EEEEEE;
  font-size: 20rpx;
  line-height: 60rpx;
}

.box {
  width: 100%;
  position: relative;
}
.warning-view{
  background: #EEEEEE;
  padding-top: 9rpx;
  padding-left: 10rpx; 
  padding-right: 13rpx;
}
.warning-img{
  width: 35rpx;
  height: 35rpx;
}
.cancle-view{
  background: #EEEEEE;
  padding-top: 11.5rpx;
  padding-right: 10rpx;
}
.cancle-img{
  width: 40rpx;
  height: 40rpx;
}
.text {
  white-space: nowrap;
  position: absolute;
  top: 0;
  font-size: 14px;
  color: #7C8EA3
}
.add-view{
  position: fixed;
  right: 40rpx;
  bottom: 150rpx;
  background-color: #EEEEEE;
  border-radius:60rpx;
  width:120rpx;
  height: 120rpx;
}
.add{
  font-size: 100rpx;
  font-weight: 50;
  position: fixed;
  right: 40rpx;
  bottom: 150rpx;
}